  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "author": {
    "name": "Ninja77030",
    "email": "n77030@outlook.com"
  },
  "scripts": {
    "start": "electron .",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:electron": "electron-builder --win --linux",
    "lint": "vue-cli-service lint"
  },
  "bin": {
    "element-lookup": "element-lookup-linux-x64/element-lookup"
  },
  "dependencies": {
    "bulma": "^0.9.4",
