    "@vue/cli-service": "~5.0.0",
    "electron": "^6.1.12",
    "electron-builder": "^23.6.0",
    "electron-installer-snap": "^5.2.0",
    "electron-packager": "^15.4.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
